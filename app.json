{
  "addons": [
    "heroku-postgresql",
    "heroku-redis"
  ],
  "buildpacks": [{
    "url": "heroku/ruby"
  }],
  "env": {
  },
  "formation": {
    "web": {
      "quantity": 1
    },
    "worker": {
      "quantity": 1
    }, 
    "test": {
      "quantity": 1,
      "size": "performance-l"
    }
  },
  "name": "cw2",
  "scripts": {
    "postdeploy": "DISABLE_DATABASE_ENVIRONMENT_CHECK=1 rails db:schema:load db:seed",
    "test-setup": "bin/rails assets:precompile"
  },
  "stack": "heroku-18"
}
